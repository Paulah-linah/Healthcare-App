{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "flutter build web --web-renderer canvaskit --base-href /",
  "outputDirectory": "build/web"
}
